<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
	<bean  id="shape" class="com.yash.SpringIOCAssignment_3.model.Shape" abstract="true"></bean>
	
	<bean  id="rectangle" class="com.yash.SpringIOCAssignment_3.model.Rectangle" parent="shape"></bean>
	<bean  id="Parallelogram" class="com.yash.SpringIOCAssignment_3.model.Parallelogram" parent="shape"></bean>
	<bean  id="Triangle" class="com.yash.SpringIOCAssignment_3.model.Triangle" parent="shape"></bean>	 	
		
</beans>
	
